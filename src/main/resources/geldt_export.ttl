@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix geldt: <http://vocab.org/transit/terms/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix wgs84_pos: <http://www.w3.org/2003/01/geo/wgs84_pos#>.
@prefix carml: <http://carml.taxonic.com/carml/> .

<#GELDTMapping> a rr:TriplesMap ;
  rml:logicalSource [
    rml:source [
       a carml:Stream ;
       carml:streamName "GELDTStream" ;
    ] ;
    rml:referenceFormulation ql:CSV
  ];

  rr:subjectMap [
    rr:template "http://geldt.com/{GLOBALEVENTID}";
    rr:class geldt:Event;
    rr:graphMap [ rr:template  "http://geldt.time.com/{DATEADDED}" ]
  ];

  rr:predicateObjectMap [
      rr:predicate geldt:type;
      rr:objectMap [
         rr:template "http://geldt.com#{EventCode}";
        ]
      ];

  rr:predicateObjectMap [
        rr:predicate geldt:rootType;
        rr:objectMap [
          rr:template "http://geldt.com#{EventRootCode}";
          ]
        ];

  rr:predicateObjectMap [
    rr:predicate geldt:actor;
     rr:objectMap [ rr:parentTriplesMap <#Actor1TriplesMap> ];
    ];

  rr:predicateObjectMap [
    rr:predicate geldt:actor;
     rr:objectMap [ rr:parentTriplesMap <#Actor2TriplesMap> ];
    ]
  .

  <#Actor1TriplesMap>
  rml:logicalSource [
      rml:source [
         a carml:Stream ;
         carml:streamName "GELDTStream" ;
      ] ;
      rml:referenceFormulation ql:CSV
    ];
  rr:subjectMap [
      rr:template "http://geldt.com/actor/{Actor1Name}";
      rr:class geldt:Actor
    ];

  rr:predicateObjectMap [
    rr:predicate geldt:actorCode;
    rr:objectMap [
      rml:reference "Actor1Code";
      ]
    ];

  rr:predicateObjectMap [
    rr:predicate geldt:actorName;
    rr:objectMap [
      rml:reference "Actor1Name";
      ]
    ];

  rr:predicateObjectMap [
      rr:predicate geldt:actorCountry;
      rr:objectMap [
        rml:reference "Actor1CountryCode";
        ]
      ];

  rr:predicateObjectMap [
      rr:predicate geldt:actorKnownGroup;
      rr:objectMap [
        rml:reference "Actor1KnownGroupCode";
        ]
      ];

  rr:predicateObjectMap [
      rr:predicate geldt:actorEthnic;
      rr:objectMap [
        rml:reference "Actor1EthnicCode";
        ]
      ];

  rr:predicateObjectMap [
      rr:predicate geldt:actorReligion;
      rr:objectMap [
        rml:reference "Actor1Religion1Code";
        ]
      ];

  rr:predicateObjectMap [
      rr:predicate geldt:actorReligion;
      rr:objectMap [
        rml:reference "Actor1Religion2Code";
        ]
      ];

  rr:predicateObjectMap [
      rr:predicate geldt:actorType;
      rr:objectMap [
        rml:reference "Actor1Type1Code";
        ]
      ];

  rr:predicateObjectMap [
        rr:predicate geldt:actorType;
        rr:objectMap [
          rml:reference "Actor1Type2Code";
          ]
        ];

  rr:predicateObjectMap [
        rr:predicate geldt:actorType;
        rr:objectMap [
          rml:reference "Actor1Type3Code";
          ]
        ]
  .

   <#Actor2TriplesMap>
   rml:logicalSource [
       rml:source [
          a carml:Stream ;
          carml:streamName "GELDTStream" ;
       ] ;
       rml:referenceFormulation ql:CSV
     ];
    rr:subjectMap [
        rr:template "http://geldt.com/actor/{Actor2Name}";
        rr:class geldt:Actor
      ];

  rr:predicateObjectMap [
    rr:predicate geldt:actorCode;
    rr:objectMap [
      rml:reference "Actor2Code";
      ]
    ];

  rr:predicateObjectMap [
    rr:predicate geldt:actorName;
    rr:objectMap [
      rml:reference "Actor2Name";
      ]
    ];

  rr:predicateObjectMap [
      rr:predicate geldt:actorCountry;
      rr:objectMap [
        rml:reference "Actor2CountryCode";
        ]
      ];

  rr:predicateObjectMap [
      rr:predicate geldt:actorKnownGroup;
      rr:objectMap [
        rml:reference "Actor2KnownGroupCode";
        ]
      ];

  rr:predicateObjectMap [
      rr:predicate geldt:actorEthnic;
      rr:objectMap [
        rml:reference "Actor2EthnicCode";
        ]
      ];

  rr:predicateObjectMap [
      rr:predicate geldt:actorReligion;
      rr:objectMap [
        rml:reference "Actor2Religion1Code";
        ]
      ];

  rr:predicateObjectMap [
      rr:predicate geldt:actorReligion;
      rr:objectMap [
        rml:reference "Actor2Religion2Code";
        ]
      ];

  rr:predicateObjectMap [
      rr:predicate geldt:actorType;
      rr:objectMap [
        rml:reference "Actor2Type1Code";
        ]
      ];

  rr:predicateObjectMap [
        rr:predicate geldt:actorType;
        rr:objectMap [
          rml:reference "Actor2Type2Code";
          ]
        ];

  rr:predicateObjectMap [
        rr:predicate geldt:actorType;
        rr:objectMap [
          rml:reference "Actor2Type3Code";
          ]
        ];






.

