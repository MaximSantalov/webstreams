@prefix : <http://vocab.org/transit/terms/>.
@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix geldt: <http://geldt.org/vocab/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix wgs84_pos: <http://www.w3.org/2003/01/geo/wgs84_pos#>.
@prefix carml: <http://carml.taxonic.com/carml/> .
@prefix vocals: <http://w3id.org/rsp/vocals#> .

<#GELDTMapping> a rr:TriplesMap ;
  rml:logicalSource [
    rml:source [
       a vocals:Stream ;
       vocals:streamName "GELDTStream" ;
    ] ;
    rml:referenceFormulation ql:CSV
  ];

  rr:subjectMap [
    rr:template "http://geldt.org/instance/{GLOBALEVENTID}";
    rr:class geldt:Mention
  ];

  rr:predicateObjectMap [
    rr:predicate geldt:mentionID;
    rr:objectMap [
      rml:reference "MentionIdentifier";
      ]
    ];


  rr:predicateObjectMap [
      rr:predicate geldt:eventDateTime;
      rr:objectMap [
         rr:template "http://geldt.com#{EventTimeDate}";
        ]
      ];


  rr:predicateObjectMap [
      rr:predicate geldt:mentionDateTime;
      rr:objectMap [
         rr:template "http://geldt.com#{MentionTimeDate}";
        ]
      ];


  rr:predicateObjectMap [
      rr:predicate geldt:mentionType;
      rr:objectMap [
         rr:template "http://geldt.org/vocab/{MentionType}";
        ]
      ];


  rr:predicateObjectMap [
      rr:predicate geldt:mentionSource;
      rr:objectMap [
         rr:template "http://geldt.org/vocab/{MentionSourceName}";
        ]
      ];


  rr:predicateObjectMap [
      rr:predicate geldt:mentionSource;
      rr:objectMap [
         rml:reference "MentionSourceName";
        ]
      ];


  rr:predicateObjectMap [
      rr:predicate geldt:sentenceID;
      rr:objectMap [
         rml:reference "SentenceID";
        ]
      ];


  rr:predicateObjectMap [
      rr:predicate geldt:actorCharOffset;
      rr:objectMap [
         rml:reference "Actor1CharOffset";
        ]
      ];
  rr:predicateObjectMap [
      rr:predicate geldt:actorCharOffset;
      rr:objectMap [
         rml:reference "Actor2CharOffset";
        ]
      ];

  rr:predicateObjectMap [
      rr:predicate geldt:actionCharOffset;
      rr:objectMap [
         rml:reference "ActionCharOffset";
        ]
      ];


  rr:predicateObjectMap [
      rr:predicate geldt:inRawText;
      rr:objectMap [
         rml:reference "InRawText";
        ]
      ];

   rr:predicateObjectMap [
       rr:predicate geldt:confidence;
       rr:objectMap [
          rml:reference "Confidence";
         ]
       ];

  rr:predicateObjectMap [
      rr:predicate geldt:mentionDocLen;
      rr:objectMap [
         rml:reference "MentionDocLen";
        ]
      ];

   rr:predicateObjectMap [
        rr:predicate geldt:mentionDocTone;
        rr:objectMap [
           rml:reference "MentionDocTone";
          ]
        ];

   rr:predicateObjectMap [
       rr:predicate geldt:mentionTranslationInfo;
       rr:objectMap [
         rml:reference "MentionDocTranslationInfo";
         ]
       ];

   rr:predicateObjectMap [
     rr:predicate geldt:mentionExtas;
     rr:objectMap [
        rml:reference "Extras";
       ]
     ];

.