<http://data.gdeltproject.org/gdeltv2/lastupdate.txt> dcat:dataset :GDELTStream .


:GDELTStream a vocals:Stream ;
             vocals:hasEndpoint :GDELTCSVStreamEndpoint ;
	         vocals:hasEndpoint :GDELTEventStreamEndpoint ;
	         vocals:hasEndpoint :GDELTMentionsStreamEndpoint ;
	         vocals:hasEndpoint :GDELTGKGStreamEndpoint ;

			 dcat:title "GDELT-DATA Stream"^^xsd:string ;
			 dcat:publisher <www.geldtproject.org>;
			 dcat:description "GDELT DATA Stream"^^xsd:string .


