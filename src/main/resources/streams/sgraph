			dcat:format frmt:CSV;
			dcat:accessURL "http://data.gdeltproject.org/gdeltv2/lastupdate.txt" .


:GDELTEventStreamEndpoint a vocals:StreamEndpoint ;
						  dcat:license <https://creativecommons.org/licenses/by-nc/4.0/> ;
					      dcat:format frmt:JSON-LD;
						  dcat:accessURL "ws://localhost:8080/events" .


